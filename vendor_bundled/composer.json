	"extra": {
		"patches-file": "../installer/composer-patches/composer-patches.json"
	},
	"config": {
		"process-timeout": 5000,
		"bin-dir": "../bin",
		"component-dir": "vendor/components",
		"platform": {
			"ext-ldap": "0",
			"ext-soap": "0",
			"php": "7.2.5"
		},
		"secure-http": false
	},
	"autoload": {
		"psr-0": {
			"BOMChecker": "../lib/core/",
			"Category": "../lib/core/",
			"DeclFilter": "../lib/core/",
			"Feed": "../lib/core/",
			"H5P": "../lib/core/",
			"IDS": "../lib/core",
			"JitFilter": "../lib/core/",
			"Math": "../lib/core/",
			"Multilingual": "../lib/core/",
			"Perms": "../lib/core/",
			"Report": "../lib/core/",
			"Scheduler": "../lib/core/",
			"Search": "../lib/core/",
			"Services": "../lib/core/",
			"Table": "../lib/core/",
			"Tiki": "../lib/core/",
			"TikiDb": "../lib/core/",
			"TikiFilter": "../lib/core/",
			"Tracker": "../lib/core/",
			"Type": "../lib/core/",
			"WikiParser": "../lib/core/",
			"WikiPlugin": "../lib/core/",
			"Text": "vendor/pear/text_wiki_mediawiki/",
			"ZendSearch": "vendor/zendframework/zendsearch/library/"
		},
		"psr-4": {
			"Tiki\\": "../lib/core/",
			"Tiki\\Lib\\": "../lib/"
		},
		"classmap": [
			"../installer/",
			"../lib/language/",
			"../lib/openpgp/",
			"../lib/rating/formula/",
			"../lib/stats",
			"../lib/tikidate.php",
			"../lib/webmail",
			"vendor/adodb/adodb-php/",
			"vendor/h5p/h5p-core",
			"vendor/h5p/h5p-editor",
			"vendor/pear/xml_rpc",
			"vendor/smarty/smarty/libs/sysplugins/smarty_security.php"
		],
		"exclude-from-classmap": [
			"../lib/openpgp/OpenPGP_Zend_Mail_Transport_Smtp.php",
			"vendor/adodb/adodb-php/adodb-active-recordx.inc.php",
			"vendor/adodb/adodb-php/adodb-xmlschema03.inc.php",
			"vendor/adodb/adodb-php/drivers/adodb-ado5.inc.php",
			"vendor/adodb/adodb-php/drivers/adodb-db2ora.inc.php",
			"vendor/adodb/adodb-php/drivers/adodb-mysqlt.inc.php",
			"vendor/adodb/adodb-php/session/adodb-session2.php",
			"vendor/adodb/adodb-php/session/old/crypt.inc.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"../lib/test/"
		]
	},
	"include-path": [
		"vendor/pear/text_wiki_mediawiki/",
		"vendor/pear/xml_rpc"
	],
	"scripts": {
		"pre-install-cmd": [
			"Tiki\\Composer\\RestoreVendors::restore",
			"Tiki\\Composer\\CleanVendorAfterVendorBundledMigration::cleanLinks"
		],
		"post-install-cmd": [
			"Tiki\\Composer\\ThemeInstaller::setup",
			"Tiki\\Composer\\BootstrapCompiler::build",
			"Tiki\\Composer\\CleanVendors::clean",
			"Tiki\\Composer\\CleanVendorAfterVendorBundledMigration::clean",
			"Tiki\\Composer\\PatchCypht::setup"
		],
		"pre-update-cmd": [
			"Tiki\\Composer\\RestoreVendors::restore",
			"Tiki\\Composer\\ThemeInstaller::setup",
			"Tiki\\Composer\\CleanVendorAfterVendorBundledMigration::cleanLinks"
		],
		"post-update-cmd": [
			"Tiki\\Composer\\BootstrapCompiler::build",
			"Tiki\\Composer\\CleanVendors::clean",
			"Tiki\\Composer\\CleanVendorAfterVendorBundledMigration::clean",
			"Tiki\\Composer\\PatchCypht::setup"
		],
		"test": "php ../lib/test/phpunit.php -c ../phpunit.xml.dist --colors=always",
		"format": "php-cs-fixer fix -vvv --config=../.php_cs.dist --ansi"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.tiki.org"
		},
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
